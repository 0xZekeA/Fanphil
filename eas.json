{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "env": {
          "CI": "1",
          "EXPO_USE_HERMES": "true"
        }
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "env": {
          "CI": "1",
          "EXPO_USE_HERMES": "true"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
